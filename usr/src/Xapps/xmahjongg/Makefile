	@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi
	$(INSTALL) -c $(INSTMANFLAGS) xmahjongg.man $(DESTDIR)$(MANDIR)/xmahjongg.$(MANSUFFIX)

