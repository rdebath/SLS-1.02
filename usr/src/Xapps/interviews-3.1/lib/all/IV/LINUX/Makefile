tif_fax3.o: g3states.h
g3states.h: $(TIFF)/mkg3states.c $(TIFF)/t4.h
	@$(RM) mkg3states
	$(TIFF_CDRIVER) $(TIFF_CLDFLAGS) -o mkg3states $(TIFF)/mkg3states.c
	./mkg3states > g3states.h
clean::
	$(RM) mkg3states g3states.h

