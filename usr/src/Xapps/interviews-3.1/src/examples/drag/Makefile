Makefiles::
	@echo "Making Makefiles" \
	"for $(ARCH) in $(CURRENT_DIR)/$(ARCH)"
	-@if [ ! -d $(ARCH) ]; then \
		mkdir $(ARCH); \
		chmod g+w $(ARCH); \
	fi; \
	if [ -f $(ARCH)/Makefile ]; then \
		$(RM) $(ARCH)/Makefile.bak; \
		$(MV) $(ARCH)/Makefile $(ARCH)/Makefile.bak; \
	fi; \
	$(IMAKE) $(IMAKEFLAGS) \
	-DTOPDIR=../$(TOP) -DCURDIR=$(CURRENT_DIR)/$(ARCH) \
	-DInObjectCodeDir -s $(ARCH)/Makefile

depend::
	-@for i in $(ARCH); \
	do \
		if [ -d $$i ]; then ( \
			echo "depending" \
			"for $(ARCH) in $(CURRENT_DIR)/$$i"; \
			cd $$i; \
			$(MAKE) $(PASSARCH) depend; \
		) else continue; fi; \
	done

all::
	-@for i in $(ARCH); \
	do \
		if [ -d $$i ]; then ( \
			echo "making all" \
			"for $(ARCH) in $(CURRENT_DIR)/$$i"; \
			cd $$i; \
			$(MAKE) $(PASSARCH) all; \
		) else continue; fi; \
	done

install::
	-@for i in $(ARCH); \
	do \
		if [ -d $$i ]; then ( \
			echo "installing" \
			"for $(ARCH) in $(CURRENT_DIR)/$$i"; \
			cd $$i; \
			$(MAKE) $(PASSARCH) install; \
		) else continue; fi; \
	done

clean::
	-@for i in $(ARCH); \
	do \
		if [ -d $$i ]; then ( \
			echo "cleaning" \
			"for $(ARCH) in $(CURRENT_DIR)/$$i"; \
			cd $$i; \
			$(MAKE) $(PASSARCH) clean; \
		) else continue; fi; \
	done

